{% extends 'base.html.twig' %}

{% block title %}Créer un film{% endblock %}

{% block body %}
    <div class="container my-5">
        <h1 class="mb-4">Créer un film</h1>

        {{ form_start(form) }}
        {{ form_widget(form) }}
        <button class="btn btn-primary">Enregistrer</button>
        {{ form_end(form) }}

        <a href="{{ path('film_index') }}" class="btn btn-link mt-3">Retour à la liste</a>
    </div>
{% endblock %}
